{
    "name": "eboseogbidi/smartpaymentrouter",
    "description": "This package can intelligently route payment transactions to the most suitable payment processor based on various factors such as transaction cost, reliability, and currency support.",
    "type": "package",
    "version": "1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eboseogbidi\\Smartpaymentrouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eboseogbidi\\Smartpaymentrouter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel":{
            "providers":[
                "Eboseogbidi\\Smartpaymentrouter\\Providers\\SmartPaymentRouterProvider" 
            ]
        }
    },
    "authors": [
        {
            "name": "EbosetaleseDunjoyin",
            "email": "eboselese@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0", 
        "phpunit/phpunit": "^9.0" 
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
